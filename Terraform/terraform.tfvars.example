# AWS Configuration
region = "us-east-1"
environment = "production"

# EKS Cluster Configuration
cluster_name = "studynotion-eks"
kubernetes_version = "1.28"

# VPC Configuration
vpc_cidr = "10.0.0.0/16"
private_subnet_cidrs = ["10.0.1.0/24", "10.0.2.0/24", "10.0.3.0/24"]
public_subnet_cidrs = ["10.0.101.0/24", "10.0.102.0/24", "10.0.103.0/24"]

# Jump Server Configuration
jump_server_ami = "ami-0c02fb55956c7d316"  # Amazon Linux 2 in us-east-1
jump_server_instance_type = "t3.micro"
key_pair_name = "your-aws-key-pair-name"  # Replace with your actual key pair name
